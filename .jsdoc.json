{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include":["README.md", "extra/", "index.js", "base/", "lib/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },

    "opts": {
        "template": "node_modules/minami",
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "tutorials": "extra/"
    }
}